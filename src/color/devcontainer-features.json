{
  "name": "My Favorite Color",
  "id": "color",
  "version": "1.0.3",
  "description": "A feature to remind you of your favorite color",
  "options": {
    "favorite": {
      "type": "string",
      "enum": [
        "red",
        "gold",
        "green"
      ],
      "default": "red",
      "description": "Choose your favorite color."
    }
  },
  "installsAfter": [
    "ghcr.io/devcontainers/features/common-utils"
  ]
}

